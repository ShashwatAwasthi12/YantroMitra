<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel='stylesheet' href='display.css'>
</head>
<body>
    <h1 id='heading'>TEACHERS PROFILE</h1>
    <div>
        
        <%    users.forEach( (user) =>{ %>
                <div id='layout'>
                    <div id='layout1'>
                        <a href='/display2/<%= user._id %>'>
                            <div id='layout1'>
                                <h3>Name:<%= user.name %></h3>
                                <p>Age:<%= user.age %></p>
                                <p>DOB:<%= user.dob%></p>
                                <p>Qualifications:<%= user.qualification%></p>
                            </div>
                        </a>
                    </div>
              
                    <div id='layout2'>
                        <img id='image' src=<%= user.image %> alt=''>
                    </div>
                </div>
               
           <% }) %>
       
    </div>
   
    

</body>
</html>