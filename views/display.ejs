<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel='stylesheet' href='display.css'>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-wEmeIV1mKuiNpC+IOBjI7aAzPcEZeedi5yW5f2yOq55WWLwNGmvvx4Um1vskeMj0" crossorigin="anonymous">
</head>

<body>

    <div class="pricing-header p-3 pb-md-4 mx-auto text-center">
        <img src="school-logo.jpg" alt="" srcset="">
        <h1 id='heading'>STUDENTS PROFILE</h1>
    </div>

    <div class="p-5 mb-4 bg-light rounded-3">
        <% users.forEach( (user)=>{ %>
            <div class="row">
                <div class="col container-fluid py-5">
                    <p class="col-md-8 fs-4">
                    <div id='layout'>
                        <div id='layout1'>

                            <h4>
                                <p><a href='/display/<%= user._id %>'>Name:<%= user.name %></a>
                                </p>
                            </h4>
                            <h3>
                                <p><a href='/display/<%= user._id %>'>Age:<%= user.age %></a>
                                </p>
                                </h4>
                                <h3>
                                    <p><a href='/display/<%= user._id %>'>DOB:<%= user.dob%></a>
                                    </p>
                                    </h4>
                                    <h4>
                                        <p><a href='/display/<%= user._id %>'>Std:<%= user.std%></a>
                                        </p>
                                    </h4>
                                    <h4>
                                        <p><a href='/display/<%= user._id %>'>Div:<%= user.div%></a>
                                        </p>
                                    </h4>
                                    <h4>
                                        <p><a href='/display/<%= user._id %>'>Rollno:<%= user.rollno%></a>
                                        </p>
                                    </h4>
                        </div>
                        </p>
                    </div>
                </div>
                <div class="col imgcol container-fluid py-5">
                    <img class='image col-md-6 float-md-end mb-3 ms-md-3' src='<%= user.image %>' alt=''>
                </div>
            </div>
            <% }) %>
    </div>



</body>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-p34f1UUtsS3wqzfto5wAAmdvj+osOnFyQFpp4Ua3gs/ZVWx6oOypYoCJhGGScy+8"
    crossorigin="anonymous"></script>

</html>